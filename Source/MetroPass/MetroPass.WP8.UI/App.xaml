<Application
    x:Class="MetroPass.WP8.UI.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:phone="clr-namespace:Microsoft.Phone.Controls;assembly=Microsoft.Phone"
    xmlns:shell="clr-namespace:Microsoft.Phone.Shell;assembly=Microsoft.Phone"
    xmlns:local="clr-namespace:MetroPass.WP8.UI"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <!--Application Resources-->

    <Application.Resources>     
        <ResourceDictionary>           
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles.xaml" />               
            </ResourceDictionary.MergedDictionaries>
            <local:LocalizedStrings xmlns:local="clr-namespace:MetroPass.WP8.UI" x:Key="LocalizedStrings"/>
            <sys:String x:Key="AppName">MetroPass</sys:String>
			<local:Bootstrapper x:Key="bootstrapper"/>
        </ResourceDictionary>
     

    </Application.Resources>

</Application>

